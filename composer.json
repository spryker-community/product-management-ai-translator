{
    "name": "spryker-community/product-management-ai-translator",
    "type": "library",
    "description": "The ProductManagementAiTranslator module provides product-related AI translation functionality",
    "keywords": ["spryker", "product-management", "AI", "translations"],
    "homepage": "https://github.com/spryker-community/product-management-ai-translator",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "spryker-community/ai-translator": "^0.1.0",
        "spryker/kernel": "^3.70.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "SprykerCommunity\\": "src/SprykerCommunity/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerCommunityTest\\": "tests/SprykerCommunityTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
